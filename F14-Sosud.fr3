<TfrxReport Version="4.8.27" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="41353,4893696412" ReportOptions.Description.Text="" ReportOptions.LastChange="41792,4063304514" ScriptLanguage="PascalScript" ScriptText.Text="&#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  S : String;&#13;&#10;  BeginYear, BeginMonth, BeginDay : integer;&#13;&#10;  EndYear,   EndMonth,   EndDay   : integer;                              &#13;&#10;  yy,        mm,         dd       : integer;&#13;&#10;  MountNames : array [1..12] of string =  ['январь', 'февраль', 'март', 'апрель', 'май', 'июнь', &#13;&#10;                                           'июль', 'август', 'сентябрь', 'октябрь', 'ноябрь', 'декабрь'];&#13;&#10;begin&#13;&#10;  S := S +  'с '  + DateToStr(dtBegin.Date);        &#13;&#10;  S := S + ' по ' + DateToStr(dtEnd.Date);&#13;&#10;  //Запомним представление периода для вывода в отчете&#13;&#10;  DecodeDate(dtBegin.Date, BeginYear, BeginMonth, BeginDay);&#13;&#10;  DecodeDate(dtEnd.Date,   EndYear,   EndMonth,   EndDay);&#13;&#10;  DecodeDate(dtEnd.Date+1, yy,        mm,         dd);                                                                                                                                                                                      &#13;&#10;  // если годы совпадают, месяцы с 1 (январь) по 12 (декабрь), дни с 1 января по 31 декабря, то отчет ЗА ГОД                                                                                                                                                                                                            &#13;&#10;  if (BeginYear = EndYear) and (BeginMonth=1) and (EndMonth=12) and (BeginDay=1) and (EndDay=31) then&#13;&#10;    S := 'за ' + FloatToStr(BeginYear) + ' год'&#13;&#10;  else   &#13;&#10;    // если годы и месяцы совпадают, начальное число = 1, дата, следующая за конечной датой, начало следующего месяца (число = 1), то отчет ЗА МЕСЯЦ                                                                                                                                                                                                                                                         &#13;&#10;    if (BeginYear = EndYear) and (BeginMonth=EndMonth) and (BeginDay=1) and (dd=1) then&#13;&#10;      S := 'за ' + MountNames[BeginMonth-1] + ' ' + IntToStr(BeginYear) + ' г'              &#13;&#10;    else                  &#13;&#10;      if DateToStr(dtBegin.Date) &#60;&#62; DateToStr(dtEnd.Date) then&#13;&#10;        S := 'за период ' + S  &#13;&#10;      else&#13;&#10;        S := 'за ' + FormatDateTime('dd.mm.yyyy', dtBegin.Date);&#13;&#10;  Set('PeriodName', ''''+S+'''');&#13;&#10;  if DBLookupComboBox1.KeyValue&#60;&#62;Null then                             // BDC 27.06.2013                                         &#13;&#10;    Set('OtdName', '''' + dsOtd.FieldByName('NaimOtd').AsString+'''')  // BDC 27.06.2013                                    &#13;&#10;  else                                                                 // BDC 27.06.2013   &#13;&#10;    Set('OtdName','''' + ' ' + '''');                                  // BDC 27.06.2013  &#13;&#10;  dsLPU.Close;&#13;&#10;  dsLPU.ParamByName('kodbol').Value := StrToInt(VarIdLPU);&#13;&#10;  dsLPU.Open;          &#13;&#10;  Set('NameMO', ''''+dsLPU.FieldByName('st_bol_name').AsString+'''');&#13;&#10;  dsLPU.Close;&#13;&#10;    &#13;&#10;  dsPage2.Close;                  &#13;&#10;  dsPage2.ParamByName('dtStart').Value := DateToStr(dtBegin.Date);            &#13;&#10;  dsPage2.ParamByName('dtStop').Value  := DateToStr(dtEnd.Date);&#13;&#10;  dsPage2.ParamByName('lpu_id').Value  := StrToInt(VarIdLPU);&#13;&#10;  if DBLookupComboBox1.KeyValue &#60;&#62; null then                           // BDC 27.06.2013&#13;&#10;    begin                                                                                // BDC 28.08.2013                                                             &#13;&#10;    //dsPage2.ParamByName('Otd').Value := DBLookupComboBox1.KeyValue   // BDC 27.06.2013 // BDC 28.08.2013  &#13;&#10;      dsPage2.ParamByName('Otd').Value := dsOtd.FieldByName('KODOTD').Value;             // BDC 28.08.2013&#13;&#10;      if dsOtd.FieldByName('OTD_NUM').Value = null then                                  // BDC 28.08.2013  &#13;&#10;        dsPage2.ParamByName('OTD_NUM').Value := 0                                        // BDC 28.08.2013  &#13;&#10;      else                                                                               // BDC 28.08.2013                                                                                                     &#13;&#10;        dsPage2.ParamByName('OTD_NUM').Value := dsOtd.FieldByName('OTD_NUM').Value;      // BDC 28.08.2013&#13;&#10;    end                                                                                  // BDC 28.08.2013              &#13;&#10;  else                                                                 // BDC 27.06.2013&#13;&#10;    begin                                                                                // BDC 28.08.2013                               &#13;&#10;      dsPage2.ParamByName('Otd').Value := 0;                           // BDC 27.06.2013&#13;&#10;      dsPage2.ParamByName('OTD_NUM').Value := 0;                                         // BDC 28.08.2013&#13;&#10;    end;                                                                                 // BDC 28.08.2013                &#13;&#10;  dsPage2.Open;&#13;&#10;  if dsPage2.RecordCount = 0 then&#13;&#10;    begin                                 &#13;&#10;      Page2.Visible       := False;&#13;&#10;//      Memo106.Height      := 0.5;&#13;&#10;//      ReportTitle3.Height := 0.5;                                    &#13;&#10;    end&#13;&#10;  else&#13;&#10;    begin&#13;&#10;      Memo106.Height      := 0;&#13;&#10;      ReportTitle3.Height := 0;                                    &#13;&#10;    end;              &#13;&#10;&#13;&#10;  dsPage4.Close;                  &#13;&#10;  dsPage4.ParamByName('dtStart').Value := DateToStr(dtBegin.Date);            &#13;&#10;  dsPage4.ParamByName('dtStop').Value  := DateToStr(dtEnd.Date);&#13;&#10;  dsPage4.ParamByName('lpu_id').Value  := StrToInt(VarIdLPU);&#13;&#10;  if DBLookupComboBox1.KeyValue &#60;&#62; null then                           // BDC 27.06.2013&#13;&#10;    begin                                                                                // BDC 28.08.2013                                                             &#13;&#10;    //dsPage4.ParamByName('Otd').Value := DBLookupComboBox1.KeyValue   // BDC 27.06.2013 // BDC 28.08.2013                                      &#13;&#10;      dsPage4.ParamByName('Otd').Value := dsOtd.FieldByName('KODOTD').Value;             // BDC 28.08.2013&#13;&#10;      if dsOtd.FieldByName('OTD_NUM').Value = null then                                  // BDC 28.08.2013  &#13;&#10;        dsPage4.ParamByName('OTD_NUM').Value := 0                                        // BDC 28.08.2013  &#13;&#10;      else                                                                               // BDC 28.08.2013                                                                                                     &#13;&#10;        dsPage4.ParamByName('OTD_NUM').Value := dsOtd.FieldByName('OTD_NUM').Value;      // BDC 28.08.2013&#13;&#10;    end                                                                                  // BDC 28.08.2013              &#13;&#10;  else                                                                 // BDC 27.06.2013   &#13;&#10;    begin                                                                                // BDC 28.08.2013                                                             &#13;&#10;      dsPage4.ParamByName('Otd').Value := 0;                           // BDC 27.06.2013  &#13;&#10;      dsPage4.ParamByName('OTD_NUM').Value := 0;                                         // BDC 28.08.2013&#13;&#10;    end;                                                                                 // BDC 28.08.2013              &#13;&#10;  dsPage4.Open;&#13;&#10;  if dsPage4.RecordCount = 0 then&#13;&#10;    Page4.Visible := False;                                                                     &#13;&#10;          &#13;&#10;  Button1.ModalResult := mrOk;                                        &#13;&#10;  fMain.ModalResult   := mrOk;    &#13;&#10;end;&#13;&#10;&#13;&#10;procedure fMainOnActivate(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  dtBegin.Date := EncodeDate(YearOf(Now),MonthOf(Now),1);&#13;&#10;  dtEnd.Date := EncodeDate(YearOf(now),MonthOf(now),DaysInMonth(YearOf(now),MonthOf(now)));&#13;&#10;  dsOtd.Close;                                              // BDC 27.06.2013  &#13;&#10;  dsOtd.ParamByName('lpu_id').Value := StrToInt(VarIdLPU);  // BDC 27.06.2013                                    &#13;&#10;  dsOtd.Open;                                               // BDC 27.06.2013  &#13;&#10;  dsOtd.Last;                                               // BDC 27.06.2013  &#13;&#10;  dsOtd.First;                                              // BDC 27.06.2013            &#13;&#10;end;&#13;&#10;    &#13;&#10;procedure PageHeader1OnAfterPrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Memo8.Memo.Text := 'продолжение';    &#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  lvl : integer;                                       &#13;&#10;begin&#13;&#10;  lvl := dsPage2.FieldByName('LVL').AsInteger;                             &#13;&#10;//Memo33.GapX := 2 + lvl * 14;&#13;&#10;  Memo33.Left   := Memo23.Width * (lvl * 3) / 100;&#13;&#10;  Memo33.Width  := Memo23.Width * (100 - lvl * 3) / 100;&#13;&#10;  Memo104.Width := Memo23.Width * (lvl * 3) / 100;&#13;&#10;  if (lvl = 0) then&#13;&#10;    Memo33.Font.Style := fsBold               &#13;&#10;  else&#13;&#10;    Memo33.Font.Style := 0;                          &#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;procedure MasterData3OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  lvl : integer;                                       &#13;&#10;begin&#13;&#10;  lvl := dsPage4.FieldByName('LVL').AsInteger;                             &#13;&#10;  Memo133.Left   := Memo123.Width * (lvl * 3) / 100;&#13;&#10;  Memo133.Width  := Memo123.Width * (100 - lvl * 3) / 100;&#13;&#10;  Memo143.Width  := Memo123.Width * (lvl * 3) / 100;&#13;&#10;  if (lvl = 0) then&#13;&#10;    Memo133.Font.Style := fsBold               &#13;&#10;  else&#13;&#10;    Memo133.Font.Style := 0;                          &#13;&#10;end;&#13;&#10;&#13;&#10;procedure btSubOnClick(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  D, M, Y: word;&#13;&#10;  LastDay: boolean;&#13;&#10;begin&#13;&#10;  DecodeDate(dtBegin.Date,Y,M,D);&#13;&#10;  LastDay := (D=DaysInMonth(Y,M));&#13;&#10;  M := M - 1;&#13;&#10;  if M = 0 then&#13;&#10;  begin&#13;&#10;    Y := Y - 1;&#13;&#10;    M := 12;&#13;&#10;  end;&#13;&#10;  if LastDay then&#13;&#10;    D := DaysInMonth(Y,M)&#13;&#10;  else&#13;&#10;  if D&#62;DaysInMonth(Y,M) then&#13;&#10;    D := DaysInMonth(Y,M);&#13;&#10;  dtBegin.Date := EncodeDate(Y,M,D);&#13;&#10;&#13;&#10;  DecodeDate(dtEnd.Date,Y,M,D);&#13;&#10;  LastDay := (D=DaysInMonth(Y,M));&#13;&#10;  M := M - 1;&#13;&#10;  if M = 0 then&#13;&#10;  begin&#13;&#10;    Y := Y - 1;&#13;&#10;    M := 12;&#13;&#10;  end;&#13;&#10;  if LastDay then&#13;&#10;    D := DaysInMonth(Y,M)&#13;&#10;  else&#13;&#10;  if D&#62;DaysInMonth(Y,M) then&#13;&#10;    D := DaysInMonth(Y,M);&#13;&#10;  dtEnd.Date := EncodeDate(Y,M,D);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure btAddOnClick(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  D, M, Y: word;&#13;&#10;  LastDay: boolean;&#13;&#10;begin&#13;&#10;  DecodeDate(dtBegin.Date,Y,M,D);&#13;&#10;  LastDay := (D=DaysInMonth(Y,M));&#13;&#10;  M := M + 1;&#13;&#10;  if M = 13 then&#13;&#10;  begin&#13;&#10;    Y := Y + 1;&#13;&#10;    M := 1;&#13;&#10;  end;&#13;&#10;  if LastDay then&#13;&#10;    D := DaysInMonth(Y,M)&#13;&#10;  else&#13;&#10;  if D&#62;DaysInMonth(Y,M) then&#13;&#10;    D := DaysInMonth(Y,M);&#13;&#10;  dtBegin.Date := EncodeDate(Y,M,D);&#13;&#10;&#13;&#10;  DecodeDate(dtEnd.Date,Y,M,D);&#13;&#10;  LastDay := (D=DaysInMonth(Y,M));&#13;&#10;  M := M + 1;&#13;&#10;  if M = 13 then&#13;&#10;  begin&#13;&#10;    Y := Y + 1;&#13;&#10;    M := 1;&#13;&#10;  end;&#13;&#10;  if LastDay then&#13;&#10;    D := DaysInMonth(Y,M)&#13;&#10;  else&#13;&#10;  if D&#62;DaysInMonth(Y,M) then&#13;&#10;    D := DaysInMonth(Y,M);&#13;&#10;  dtEnd.Date := EncodeDate(Y,M,D);&#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;begin&#13;&#10;   &#13;&#10;end." StoreInDFM="False" OnReportPrint="frxReportOnReportPrint" PropData="">
