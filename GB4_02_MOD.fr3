<TfrxReport Version="4.8.27" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="40720,5875882407" ReportOptions.Description.Text="" ReportOptions.LastChange="41789,4064275463" ScriptLanguage="PascalScript" ScriptText.Text="    &#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  S         : String;&#13;&#10;  i         : integer;&#13;&#10;  err       : integer;&#13;&#10;begin&#13;&#10;  err := 0;                             &#13;&#10;  S := 'за период';&#13;&#10;  S := S + ' с '  + DateToStr(dtBegin.Date);        &#13;&#10;  S := S + ' по ' + DateToStr(dtEnd.Date);&#13;&#10;  Set('PeriodName', '''' + S + '''');&#13;&#10;  if DBLookupComboBox1.KeyValue&#60;&#62;Null then                                       &#13;&#10;    Set('OtdName', '''' + dsOtd.FieldByName('NaimOtd').AsString+'''')&#13;&#10;  else &#13;&#10;    Set('OtdName', '''' + 'по больнице в целом' + '''');&#13;&#10;  dsLPU.Close;&#13;&#10;  dsLPU.ParamByName('kodbol').Value := StrToInt(VarIdLPU);&#13;&#10;  dsLPU.Open;          &#13;&#10;  Set('NameMO', '''' + dsLPU.FieldByName('st_bol_name').AsString + '''');&#13;&#10;  dsLPU.Close;&#13;&#10;  dsData.Close;                  &#13;&#10;  dsData.ParamByName('dtStart').Value := DateToStr(dtBegin.Date);&#13;&#10;  dsData.ParamByName('dtStop').Value  := DateToStr(dtEnd.Date);&#13;&#10;  dsData.ParamByName('ID_LPU').Value  := StrToInt(VarIdLPU);&#13;&#10;  if DBLookupComboBox1.KeyValue &#60;&#62; null then&#13;&#10;    begin                                                                // BDC 29.08.2013                           &#13;&#10;    //dsData.ParamByName('Otd').Value := DBLookupComboBox1.KeyValue      // BDC 29.08.2013  &#13;&#10;      dsData.ParamByName('KOD_NUM').Value := DBLookupComboBox1.KeyValue; // BDC 29.08.2013&#13;&#10;      ReportSummary1.Visible := False;                                   // BDC 29.08.2013                                                  &#13;&#10;    end                                                                  // BDC 29.08.2013                      &#13;&#10;  else&#13;&#10;    begin                                                                // BDC 29.08.2013                           &#13;&#10;    //dsData.ParamByName('Otd').Value := 0;                              // BDC 29.08.2013  &#13;&#10;      dsData.ParamByName('KOD_NUM').Value := 0;                          // BDC 29.08.2013  &#13;&#10;      ReportSummary1.Visible := True;                                    // BDC 29.08.2013                                                  &#13;&#10;    end;                                                                 // BDC 29.08.2013                  &#13;&#10;  dsData.Open;&#13;&#10;  dsData.Last;&#13;&#10;  dsData.First;&#13;&#10;  if dsData.RecordCount = 0 then&#13;&#10;    begin&#13;&#10;      ShowMessage('Нет данных согласно выбранным условиям!');&#13;&#10;      err := 1;                            &#13;&#10;    end;  &#13;&#10;  if err = 0 then&#13;&#10;    begin&#13;&#10;      Button1.ModalResult := mrOk;                                        &#13;&#10;      fMain.modalresult   := mrOk;    &#13;&#10;    end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure fMainOnActivate(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  ttt : string;                                                       &#13;&#10;begin&#13;&#10;  dtBegin.Date := EncodeDate(YearOf(now),MonthOf(now),1);&#13;&#10;  dtEnd.Date := now;&#13;&#10;  dsOtd.Close;&#13;&#10;  dsOtd.ParamByName('lpu_id').Value := StrToInt(VarIdLPU);&#13;&#10;  dsOtd.Open;&#13;&#10;  dsOtd.Last;&#13;&#10;  dsOtd.First;          &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." StoreInDFM="False" OnReportPrint="frxReportOnReportPrint" PropData="">
  <TfrxDataPage Name="Data" Height="1000" Left="0" Top="0" Width="1000">
    <TfrxFIBQuery Name="dsData" UserName="dsData" CloseDataSource="True" FieldAliases.Text="KODOTD=KODOTD&#13;&#10;KOD_NUM=KOD_NUM&#13;&#10;NAIMOTD=NAIMOTD&#13;&#10;NIB=NIB&#13;&#10;FIO=FIO&#13;&#10;DT_RECEIPT=DT_RECEIPT&#13;&#10;DTRECEPT=DTRECEPT&#13;&#10;DTEXTRACT=DTEXTRACT&#13;&#10;KD=KD&#13;&#10;KDR=KDR&#13;&#10;SEX=SEX&#13;&#10;FULL_YEAR=FULL_YEAR&#13;&#10;ADR=ADR&#13;&#10;WORK_PLACE=WORK_PLACE&#13;&#10;CLINIC_DIAG=CLINIC_DIAG&#13;&#10;PATOLOG_DIAG=PATOLOG_DIAG&#13;&#10;SOV=SOV&#13;&#10;HOSP_EXTRPOK=HOSP_EXTRPOK&#13;&#10;SPRDOC_ID_FK=SPRDOC_ID_FK&#13;&#10;SRV_ID=SRV_ID" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text=" SELECT&#13;&#10;       KODOTD,&#13;&#10;       KOD_NUM,                               -- BDC 29.08.2013                                                                              &#13;&#10;       NAIMOTD,&#13;&#10;       NIB,&#13;&#10;       FIO,&#13;&#10;       DT_RECEIPT,&#13;&#10;       DTRECEPT,&#13;&#10;       DTEXTRACT,&#13;&#10;       KD,&#13;&#10;       KDR,&#13;&#10;       SEX,&#13;&#10;       FULL_YEAR,&#13;&#10;       ADR,&#13;&#10;       WORK_PLACE,&#13;&#10;       CLINIC_DIAG,&#13;&#10;       PATOLOG_DIAG,&#13;&#10;       SOV,&#13;&#10;       HOSP_EXTRPOK,&#13;&#10;       SPRDOC_ID_FK,&#13;&#10;       SRV_ID&#13;&#10;   FROM RP_GB4_02(:ID_LPU,  :dtStart, :dtStop)&#13;&#10;--WHERE (KODOTD = :otd OR :otd  = 0)             &#13;&#10;  WHERE (KOD_NUM = :KOD_NUM OR :KOD_NUM  = 0)&#13;&#10;  AND (((FULL_YEAR &#62;= 55)AND(SEX = 'Ж')) OR ((FULL_YEAR &#62;= 60)AND(SEX = 'М')))                              &#13;&#10;--ORDER BY 1,3,6                                            &#13;&#10;  ORDER BY 2,4,7                                           &#13;&#10;      " PropData=""/>
