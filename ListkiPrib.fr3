                          &#13;&#10;    DischargedCount.ParamByName('KOD_NUM').Value := 0;      &#13;&#10;  DischargedCount.Open;&#13;&#10;&#13;&#10;     &#13;&#10;  cnt:=DischargedCount.FieldByName('COUNT').Value;                    &#13;&#10;  if cnt MOD 4 = 0&#13;&#10;       then n:=cnt DIV 4&#13;&#10;       else n:=cnt DIV 4 + 1;                               &#13;&#10;  &#13;&#10;  ShowMessage(IntToStr(cnt) + ' листков прибытия на ' + IntToStr(n) +' листах А4');&#13;&#10;          &#13;&#10;  Button1.ModalResult:=mrOk;                                        &#13;&#10;  fMain.modalresult:=mrOk;    &#13;&#10;end;&#13;&#10;&#13;&#10;procedure fMainOnActivate(Sender: TfrxComponent);&#13;&#10;begin       &#13;&#10;  if (Dayof(now)=1)and(MonthOf(now)=1)&#13;&#10;       then dtBegin.Date := EncodeDate(YearOf(now)-1, 12, 31)              &#13;&#10;  else if Dayof(now)=1 &#13;&#10;       then dtBegin.Date := EncodeDate(YearOf(now), MonthOf(now)-1, DaysInMonth(YearOf(now),MonthOf(now)-1))&#13;&#10;  else dtBegin.Date := EncodeDate(YearOf(now), MonthOf(now), Dayof(now)-1);&#13;&#10;&#13;&#10;  dtEnd.Date := EncodeDate(YearOf(now), MonthOf(now), Dayof(now)-1);&#13;&#10;        &#13;&#10;  dsOtd.Close;                                                             &#13;&#10;  dsOtd.ParamByName('lpu_id').Value := StrToInt(VarIdLPU);                                                                                     &#13;&#10;  dsOtd.Open;                                                              &#13;&#10;  dsOtd.Last;                                                              &#13;&#10;  dsOtd.First;                                                  &#13;&#10;end;  &#13;&#10;           &#13;&#10;  &#13;&#10;procedure Page1OnManualBuild(Sender: TfrxComponent);                       &#13;&#10;var i,j,k,m:integer;&#13;&#10;begin         &#13;&#10;  i:=0;  &#13;&#10;  for j:=1 to n do     //Повторить для каждой из n страниц                                                                           &#13;&#10;       begin                   &#13;&#10;       Discharged.First;&#13;&#10;       for k:=1 to j-1 do&#13;&#10;               Discharged.Next;      //Сдвинуть указатель на текущее число страниц&#13;&#10;                 &#13;&#10;       for m := 0 to 3 do                                          &#13;&#10;               begin                                                      &#13;&#10;                 if j + m * n &#62; cnt then &#13;&#10;                       begin                           &#13;&#10;                       Engine.ShowBand(MasterData3);&#13;&#10;                       break;&#13;&#10;                       end;&#13;&#10;                         &#13;&#10;                 Patient.Close;&#13;&#10;                 Patient.ParamByName('WT_PATIENT_ID').Value := Discharged.FieldByName('WT_PATIENT_ID').Value;                                                         &#13;&#10;                 Patient.Open;                     &#13;&#10;                 Adress.Close;                                                                           &#13;&#10;                 Adress.ParamByName('KOD_KLADR').Value := Patient.FieldByName('KODKLADR_REG').Value;                                                                                                                                                     &#13;&#10;                 Adress.Open;                       &#13;&#10;                                                    &#13;&#10;                 if m = 0 then begin           &#13;&#10;                                       Line1.Visible := true;&#13;&#10;                                       Line2.Visible := true;&#13;&#10;                                       end;                                                 &#13;&#10;                 if m = 1 then begin           &#13;&#10;                                       Line1.Visible := true;&#13;&#10;                                       Line2.Visible := false;&#13;&#10;                                       end;&#13;&#10;                 if m = 2 then begin           &#13;&#10;                                       Line1.Visible := false;&#13;&#10;                                       Line2.Visible := true;&#13;&#10;                                       end;&#13;&#10;                 if m = 3 then begin               &#13;&#10;                                       Line1.Visible := false;&#13;&#10;                                       Line2.Visible := false;&#13;&#10;                                       end;                            &#13;&#10;                               &#13;&#10;                 Engine.ShowBand(MasterData1);    &#13;&#10;                 inc(i);                                                   &#13;&#10;                                                                                 &#13;&#10;                 for k:=1 to n do Discharged.Next;&#13;&#10;               end;                                                          &#13;&#10;       end;                 &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." StoreInDFM="False" OnStartReport="rAllOnStartReport" OnReportPrint="frxReportOnReportPrint" PropData="">
