<TfrxReport Version="4.8.27" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="41890,4728039699" ReportOptions.Description.Text="" ReportOptions.LastChange="41891,5725157523" ScriptLanguage="PascalScript" ScriptText.Text="procedure fMainOnActivate(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;//  dtBegin.Date := EncodeDate(YearOf(now),MonthOf(now),1);&#13;&#10;  dtEnd.Date := EncodeDate(YearOf(now),MonthOf(now),DaysInMonth(YearOf(now),MonthOf(now)));&#13;&#10;&#13;&#10;  dsOtd.Close;&#13;&#10;  dsOtd.ParamByName('lpu_id').Value := StrToInt(VarIdLPU);&#13;&#10;  dsOtd.Open;&#13;&#10;  dsOtd.Last;&#13;&#10;  dsOtd.First;&#13;&#10;&#13;&#10;  DBLookupComboBox1.KeyValue := '30';                &#13;&#10;end;&#13;&#10;  &#13;&#10;procedure btSubOnClick(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  D, M, Y: word;&#13;&#10;  LastDay: boolean;&#13;&#10;begin&#13;&#10;  DecodeDate(dtBegin.Date,Y,M,D);&#13;&#10;  LastDay := (D=DaysInMonth(Y,M));&#13;&#10;  M := M - 1;&#13;&#10;  if M = 0 then&#13;&#10;  begin&#13;&#10;    Y := Y - 1;&#13;&#10;    M := 12;&#13;&#10;  end;&#13;&#10;  if LastDay then&#13;&#10;    D := DaysInMonth(Y,M)&#13;&#10;  else&#13;&#10;  if D&#62;DaysInMonth(Y,M) then&#13;&#10;    D := DaysInMonth(Y,M);&#13;&#10;  dtBegin.Date := EncodeDate(Y,M,D);&#13;&#10;&#13;&#10;  DecodeDate(dtEnd.Date,Y,M,D);&#13;&#10;  LastDay := (D=DaysInMonth(Y,M));&#13;&#10;  M := M - 1;&#13;&#10;  if M = 0 then&#13;&#10;  begin&#13;&#10;    Y := Y - 1;&#13;&#10;    M := 12;&#13;&#10;  end;&#13;&#10;  if LastDay then&#13;&#10;    D := DaysInMonth(Y,M)&#13;&#10;  else&#13;&#10;  if D&#62;DaysInMonth(Y,M) then&#13;&#10;    D := DaysInMonth(Y,M);&#13;&#10;  dtEnd.Date := EncodeDate(Y,M,D);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure btAddOnClick(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  D, M, Y: word;&#13;&#10;  LastDay: boolean;&#13;&#10;begin&#13;&#10;  DecodeDate(dtBegin.Date,Y,M,D);&#13;&#10;  LastDay := (D=DaysInMonth(Y,M));&#13;&#10;  M := M + 1;&#13;&#10;  if M = 13 then&#13;&#10;  begin&#13;&#10;    Y := Y + 1;&#13;&#10;    M := 1;&#13;&#10;  end;&#13;&#10;  if LastDay then&#13;&#10;    D := DaysInMonth(Y,M)&#13;&#10;  else&#13;&#10;  if D&#62;DaysInMonth(Y,M) then&#13;&#10;    D := DaysInMonth(Y,M);&#13;&#10;  dtBegin.Date := EncodeDate(Y,M,D);&#13;&#10;&#13;&#10;  DecodeDate(dtEnd.Date,Y,M,D);&#13;&#10;  LastDay := (D=DaysInMonth(Y,M));&#13;&#10;  M := M + 1;&#13;&#10;  if M = 13 then&#13;&#10;  begin&#13;&#10;    Y := Y + 1;&#13;&#10;    M := 1;&#13;&#10;  end;&#13;&#10;  if LastDay then&#13;&#10;    D := DaysInMonth(Y,M)&#13;&#10;  else&#13;&#10;  if D&#62;DaysInMonth(Y,M) then&#13;&#10;    D := DaysInMonth(Y,M);&#13;&#10;  dtEnd.Date := EncodeDate(Y,M,D);&#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;  &#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);&#13;&#10;var S, otd : String;&#13;&#10;      &#13;&#10;begin&#13;&#10;  S := 'за период с ' + DateToStr(dtBegin.Date)+ ' по ' + DateToStr(dtEnd.Date);&#13;&#10;  Set('PeriodName', '''' + S + '''');&#13;&#10;                &#13;&#10;  dsLPU.Close;&#13;&#10;  dsLPU.ParamByName('kodbol').Value := StrToInt(VarIdLPU);&#13;&#10;  dsLPU.Open;          &#13;&#10;  Set('NameMO', '''' + dsLPU.FieldByName('st_bol_name').AsString + '''');&#13;&#10;  dsLPU.Close;&#13;&#10;&#13;&#10;  otd := Copy(DBLookupComboBox1.KeyValue, 0, Length(DBLookupComboBox1.KeyValue)-1);                  &#13;&#10;&#13;&#10;  case DBLookupComboBox1.KeyValue of&#13;&#10;  30:  begin    // Хирургия                    &#13;&#10;  {     dsHIRData8_1.Close;                  &#13;&#10;       dsHIRData8_1.ParamByName('dtStart').Value    := DateToStr(dtBegin.Date);            &#13;&#10;       dsHIRData8_1.ParamByName('dtStop').Value     := DateToStr(dtEnd.Date);&#13;&#10;       dsHIRData8_1.ParamByName('LPU_ID').Value     := StrToInt(VarIdLPU);&#13;&#10;       dsHIRData8_1.ParamByName('Otd').Value        := otd;                 &#13;&#10;       dsHIRData8_1.Open;&#13;&#10;&#13;&#10;       dsHIRData8_2.Close;                  &#13;&#10;       dsHIRData8_2.ParamByName('dtStart').Value    := DateToStr(dtBegin.Date);            &#13;&#10;       dsHIRData8_2.ParamByName('dtStop').Value     := DateToStr(dtEnd.Date);&#13;&#10;       dsHIRData8_2.ParamByName('LPU_ID').Value     := StrToInt(VarIdLPU);&#13;&#10;       dsHIRData8_2.ParamByName('Otd').Value        := otd;             &#13;&#10;       dsHIRData8_2.Open;    }&#13;&#10;&#13;&#10;       dsHIRData9_1.Close;                  &#13;&#10;       dsHIRData9_1.ParamByName('dtStart').Value    := DateToStr(dtBegin.Date);            &#13;&#10;       dsHIRData9_1.ParamByName('dtStop').Value     := DateToStr(dtEnd.Date);&#13;&#10;       dsHIRData9_1.ParamByName('LPU_ID').Value     := StrToInt(VarIdLPU);&#13;&#10;       dsHIRData9_1.ParamByName('Otd').Value        := otd;                 &#13;&#10;       dsHIRData9_1.Open;  &#13;&#10;             &#13;&#10;       end;                       &#13;&#10;  770: begin    // Гнойная хирургия                    &#13;&#10;       dsHIRData8_1.Close;                  &#13;&#10;       dsHIRData8_1.ParamByName('dtStart').Value    := DateToStr(dtBegin.Date);            &#13;&#10;       dsHIRData8_1.ParamByName('dtStop').Value     := DateToStr(dtEnd.Date);&#13;&#10;       dsHIRData8_1.ParamByName('LPU_ID').Value     := StrToInt(VarIdLPU);&#13;&#10;       dsHIRData8_1.ParamByName('Otd').Value        := otd;             &#13;&#10;       dsHIRData8_1.Open;&#13;&#10;&#13;&#10;       dsHIRData8_2.Close;                  &#13;&#10;       dsHIRData8_2.ParamByName('dtStart').Value    := DateToStr(dtBegin.Date);            &#13;&#10;       dsHIRData8_2.ParamByName('dtStop').Value     := DateToStr(dtEnd.Date);&#13;&#10;       dsHIRData8_2.ParamByName('LPU_ID').Value     := StrToInt(VarIdLPU);&#13;&#10;       dsHIRData8_2.ParamByName('Otd').Value        := otd;             &#13;&#10;       dsHIRData8_2.Open;      &#13;&#10;       end;      &#13;&#10;  40:  ShowMessage('Гин');                        &#13;&#10;  end;          &#13;&#10;    &#13;&#10;      &#13;&#10;  Button1.ModalResult := mrOk;                                                                                &#13;&#10;  fMain.modalresult   := mrOk;  &#13;&#10;&#13;&#10;  &#13;&#10;{             &#13;&#10;if dsHIRData.RecordCount = 0 then&#13;&#10;    begin                                                            &#13;&#10;      ShowMessage('Нет данных согласно выбранным условиям!');                                                          &#13;&#10;    end                                                                   &#13;&#10;  else                                                                    &#13;&#10;    begin                                                                 &#13;&#10;      Button1.ModalResult := mrOk;                                                                                &#13;&#10;      fMain.modalresult   := mrOk;                                            &#13;&#10;    end;  }  &#13;&#10;            &#13;&#10;end;&#13;&#10;&#13;&#10;  &#13;&#10;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." StoreInDFM="False" OnReportPrint="frxReportOnReportPrint" PropData="">
  <TfrxDataPage Name="Data" Height="1000" Left="0" Top="0" Width="1000">
    <TfrxFIBQuery Name="dsHIRData8_1" UserName="dsHIRData8_1" CloseDataSource="True" FieldAliases.Text="COUNT=COUNT" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="                                          &#13;&#10;SELECT  count(*)     /*      &#13;&#10;       KODOTD,&#13;&#10;       NAIMOTD,                         &#13;&#10;       NUMCARD,  &#13;&#10;       DT_RECEIPT,&#13;&#10;       DTEXTRACT,  &#13;&#10;       SRV_ID_FK,  &#13;&#10;       SRV_NAME,  &#13;&#10;       DTOPER,  &#13;&#10;       TMOPER     */    &#13;&#10;                               &#13;&#10;FROM&#13;&#10;   VW_WT_SURGICAL_OPERATION as op            &#13;&#10;INNER JOIN                       &#13;&#10;   VW_WT_PATIENT as pat    &#13;&#10;ON op.WT_PATIENT_ID_FK = pat.WT_PATIENT_ID                         &#13;&#10;WHERE pat.DTEXTRACT BETWEEN :dtStart AND :dtStop&#13;&#10;  AND pat.ID_LPU_FK = :LPU_ID&#13;&#10;  AND KODOTD = :Otd                                   &#13;&#10;  AND SRV_ID_FK &#60;&#62; 'A16.16.017.009'&#13;&#10;  AND (   (CLINIC_DIAG &#62;= 'K25.0' AND CLINIC_DIAG &#60;= 'K25.6')&#13;&#10;       OR (CLINIC_DIAG &#62;= 'K26.0' AND CLINIC_DIAG &#60;= 'K26.6'))        &#13;&#10;                &#13;&#10;--ORDER BY op.KODOTD, pat.DT_RECEIPT, pat.NUMCARD, op.DTOPER              " PropData=""/>
    <TfrxFIBQuery Name="dsLPU" UserName="dsLPU" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="Select St_bol_name, OGRN From VW_ST_BOL&#13;&#10;Where kodbol=:kodbol                                             " PropData="05704C65667402520470546F7002540A506172616D657465727301010622204E616D653D226B6F64626F6C222044617461547970653D226674537472696E67220000"/>
    <TfrxFIBQuery Name="dsHIRData8_2" UserName="dsHIRData8_2" CloseDataSource="True" FieldAliases.Text="COUNT=COUNT" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="                                          &#13;&#10;SELECT  count(*)     /*      &#13;&#10;       KODOTD,&#13;&#10;       NAIMOTD,                         &#13;&#10;       NUMCARD,  &#13;&#10;       DT_RECEIPT,&#13;&#10;       DTEXTRACT,  &#13;&#10;       SRV_ID_FK,  &#13;&#10;       SRV_NAME,  &#13;&#10;       DTOPER,  &#13;&#10;       TMOPER     */    &#13;&#10;                               &#13;&#10;FROM&#13;&#10;   VW_WT_SURGICAL_OPERATION as op            &#13;&#10;INNER JOIN                       &#13;&#10;   VW_WT_PATIENT as pat    &#13;&#10;ON op.WT_PATIENT_ID_FK = pat.WT_PATIENT_ID                         &#13;&#10;WHERE pat.DTEXTRACT BETWEEN :dtStart AND :dtStop&#13;&#10;  AND pat.ID_LPU_FK = :LPU_ID&#13;&#10;  AND KODOTD = :Otd      &#13;&#10;  AND (   (CLINIC_DIAG &#62;= 'K25.0' AND CLINIC_DIAG &#60;= 'K25.6')&#13;&#10;       OR (CLINIC_DIAG &#62;= 'K26.0' AND CLINIC_DIAG &#60;= 'K26.6'))        &#13;&#10;                &#13;&#10;--ORDER BY op.KODOTD, pat.DT_RECEIPT, pat.NUMCARD, op.DTOPER              " PropData=""/>
    <TfrxFIBQuery Name="dsOtd" UserName="dsOtd" CloseDataSource="True" FieldAliases.Text="KODOTD=KODOTD&#13;&#10;OTD_NUM=OTD_NUM&#13;&#10;KOD_NUM=KOD_NUM&#13;&#10;NAIMOTD=NAIMOTD&#13;&#10;VIDSTAC=VIDSTAC&#13;&#10;FULLNAME=FULLNAME" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="/* BDC 30.08.2013                                    &#13;&#10;SELECT&#13;&#10;    VW_ST_SPROTD.KODOTD,&#13;&#10;    VW_ST_SPROTD.NAIMOTD,&#13;&#10;    Left((VW_ST_SPROTD.KODOTD||'   '),3) ||' | '|| VW_ST_SPROTD.NAIMOTD as FullName&#13;&#10;from vw_st_sprotd&#13;&#10;   inner join vw_spr_visible on (vw_st_sprotd.table_name = vw_spr_visible.table_name) &#13;&#10;   and (vw_st_sprotd.KODOTD = vw_spr_visible.table_id_fk)&#13;&#10;where vw_spr_visible.lpu_id_fk = :lpu_id&#13;&#10;Order by VW_ST_SPROTD.NAIMOTD&#13;&#10;*/    &#13;&#10;SELECT *&#13;&#10;  FROM multy_otd(:lpu_id)&#13;&#10; ORDER BY naimotd&#13;&#10;    " PropData="05704C656674027C0470546F7002540A506172616D657465727301010623204E616D653D226C70755F6964222044617461547970653D226674556E6B6E6F776E2200010623204E616D653D226C70755F6964222044617461547970653D226674556E6B6E6F776E220000"/>
    <TfrxFIBQuery Name="dsHIRData9_1" UserName="dsHIRData9_1" CloseDataSource="True" FieldAliases.Text="KODOTD=KODOTD&#13;&#10;NAIMOTD=NAIMOTD&#13;&#10;NUMCARD=NUMCARD&#13;&#10;DT_RECEIPT=DT_RECEIPT&#13;&#10;TM_RECEIPT=TM_RECEIPT&#13;&#10;DTEXTRACT=DTEXTRACT&#13;&#10;SRV_ID_FK=SRV_ID_FK&#13;&#10;SRV_NAME=SRV_NAME&#13;&#10;CLINIC_DIAG=CLINIC_DIAG&#13;&#10;DTOPER=DTOPER&#13;&#10;TMOPER=TMOPER&#13;&#10;DTMOPER=DTMOPER&#13;&#10;DTREC=DTREC&#13;&#10;RAZN=RAZN" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="                                          &#13;&#10;SELECT  --count(*)     &#13;&#10;         &#13;&#10;       KODOTD,&#13;&#10;       NAIMOTD,                         &#13;&#10;       NUMCARD,  &#13;&#10;       DT_RECEIPT,&#13;&#10;	      TM_RECEIPT,&#13;&#10;       DTEXTRACT,  &#13;&#10;       SRV_ID_FK,  &#13;&#10;       SRV_NAME, &#13;&#10;	      CLINIC_DIAG,	   &#13;&#10;       DTOPER,  &#13;&#10;       TMOPER,&#13;&#10;       TMOPER + (DTOPER - CAST('1899-12-30' AS DATE)) AS DTMOPER,   &#13;&#10;       TM_RECEIPT + (DT_RECEIPT - CAST('1899-12-30' AS DATE)) AS DTREC,&#13;&#10;       (TMOPER + (DTOPER - CAST('1899-12-30' AS DATE))   -   (TM_RECEIPT + (DT_RECEIPT - CAST('1899-12-30' AS DATE))))*1441 AS RAZN                                                           &#13;&#10;       --CAST(DTOPER AS TIMESTAMP) AS t                                                                                           &#13;&#10;                               &#13;&#10;FROM&#13;&#10;   VW_WT_SURGICAL_OPERATION as op            &#13;&#10;INNER JOIN                       &#13;&#10;   VW_WT_PATIENT as pat    &#13;&#10;ON op.WT_PATIENT_ID_FK = pat.WT_PATIENT_ID                         &#13;&#10;WHERE pat.DTEXTRACT BETWEEN :dtStart AND :dtStop&#13;&#10;  AND pat.ID_LPU_FK = :LPU_ID&#13;&#10;  AND KODOTD = :Otd                                   &#13;&#10;  AND CLINIC_DIAG LIKE 'K35%'&#13;&#10;  AND (TMOPER + (DTOPER - CAST('1899-12-30' AS DATE))   -   (TM_RECEIPT + (DT_RECEIPT - CAST('1899-12-30' AS DATE))))*1441 &#60; 360                                   &#13;&#10;              &#13;&#10;                &#13;&#10;--ORDER BY op.KODOTD, pat.DT_RECEIPT, pat.NUMCARD, op.DTOPER&#13;&#10;                 " PropData=""/>
