    <TfrxFIBQuery Name="dsData" UserName="dsData" CloseDataSource="True" FieldAliases.Text="COUNT=COUNT" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="                                          &#13;&#10;SELECT  count(*)     /*      &#13;&#10;       KODOTD,&#13;&#10;       NAIMOTD,                         &#13;&#10;       NUMCARD,  &#13;&#10;       DT_RECEIPT,&#13;&#10;       DTEXTRACT,  &#13;&#10;       SRV_ID_FK,  &#13;&#10;       SRV_NAME,  &#13;&#10;       DTOPER,  &#13;&#10;       TMOPER     */    &#13;&#10;                               &#13;&#10;FROM&#13;&#10;   VW_WT_SURGICAL_OPERATION as op            &#13;&#10;INNER JOIN                       &#13;&#10;   VW_WT_PATIENT as pat    &#13;&#10;ON op.WT_PATIENT_ID_FK = pat.WT_PATIENT_ID                         &#13;&#10;WHERE pat.DTEXTRACT BETWEEN :dtStart AND :dtStop&#13;&#10;  AND pat.ID_LPU_FK = :LPU_ID&#13;&#10;  AND SRV_ID_FK &#60;&#62; 'A16.16.017.009'&#13;&#10;  AND (   (CLINIC_DIAG &#62;= 'K25.0' AND CLINIC_DIAG &#60;= 'K25.6')&#13;&#10;       OR (CLINIC_DIAG &#62;= 'K26.0' AND CLINIC_DIAG &#60;= 'K26.6'))        &#13;&#10;                &#13;&#10;--ORDER BY op.KODOTD, pat.DT_RECEIPT, pat.NUMCARD, op.DTOPER              " PropData="05704C65667403BC000470546F7002540A506172616D657465727301010624204E616D653D2264745374617274222044617461547970653D226674556E6B6E6F776E2200010623204E616D653D22647453746F70222044617461547970653D226674556E6B6E6F776E2200010623204E616D653D224C50555F4944222044617461547970653D226674556E6B6E6F776E220000"/>
