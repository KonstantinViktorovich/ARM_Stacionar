OUT, ' +                                           // BDC 30.08.2013                                                                                                                                                                                                                                                                                                                               &#13;&#10;               'NAIMOTD, ' +          &#13;&#10;               'SRV_ID        AS KOD, ' +          &#13;&#10;               'SRV_NAME      AS NAME, ' +&#13;&#10;               'COUNT(*)      AS NUM_OPER, ' +                                                     &#13;&#10;               'SUM(NUM_OSXI) AS NUM_OSXI ' +          &#13;&#10;             //'FROM RP_GB4_09A(:ID_LPU, :KODOTD, :dtStart, :dtStop) ' +   // BDC 30.08.2013  &#13;&#10;             //'FROM RP_GB4_09A(:ID_LPU, :KOD_NUM, :dtStart, :dtStop) ' +  // BDC 30.08.2013 // BDC 18.07.2014                                             &#13;&#10;               'FROM RP_GB4_09A(:ID_LPU, :KOD_NUM, :dtStart, :dtStop, :VIDOPL) ' +           // BDC 18.07.2014                                             &#13;&#10;         'WHERE SRV_ID IS NOT NULL ' +                                                                                       &#13;&#10;       //'GROUP BY 1,2,3,4 ' +                                             // BDC 30.08.2013                          &#13;&#10;         'GROUP BY 1,2,3,4,5 ' +                                           // BDC 30.08.2013                          &#13;&#10;       //'ORDER BY 1,3 ';                                                  // BDC 30.08.2013           &#13;&#10;         'ORDER BY 2,4 ';                                                  // BDC 30.08.2013           &#13;&#10;      dsDataB.SQL.Text :=&#13;&#10;        'SELECT ' +           &#13;&#10;               'SPROTD_ID_FK, ' +          &#13;&#10;               'KOD_NUM_OUT, ' +                                           // BDC 30.08.2013                                                                                                                                                                                                                                                                                                                               &#13;&#10;               'SRV_ID        AS KOD, ' +          &#13;&#10;               'SRV_NAME      AS NAME, ' +&#13;&#10;               'NIB, ' +          &#13;&#10;               'FIO, ' +          &#13;&#10;               'KODOSXI, ' +          &#13;&#10;               'NAIMOSXI ' +          &#13;&#10;             //'FROM RP_GB4_09B(:ID_LPU, :KODOTD, :dtStart, :dtStop) ' +   // BDC 30.08.2013  &#13;&#10;             //'FROM RP_GB4_09B(:ID_LPU, :KOD_NUM, :dtStart, :dtStop) ' +  // BDC 30.08.2013 // BDC 18.07.2014    &#13;&#10;               'FROM RP_GB4_09B(:ID_LPU, :KOD_NUM, :dtStart, :dtStop, :VIDOPL) ' +           // BDC 18.07.2014      &#13;&#10;       //'ORDER BY 5 ';                                                    // BDC 30.08.2013           &#13;&#10;         'ORDER BY 6 ';                                                    // BDC 30.08.2013           &#13;&#10;    end&#13;&#10;  else&#13;&#10;    begin&#13;&#10;      Memo6.Width    := Memo6Width * 0.7;&#13;&#10;      Memo5.Width    := Memo5Width + Memo6Width * 0.3;&#13;&#10;      Memo5.Left     := Memo6.Width;&#13;&#10;      Memo54.Width   := Memo6Width * 0.7;&#13;&#10;      Memo54A.Width  := Memo5Width + Memo6Width * 0.3;&#13;&#10;      Memo54A.Left   := Memo6.Width;&#13;&#10;      dsData.SQL.Text :=&#13;&#10;        'SELECT ' +           &#13;&#10;               'SPROTD_ID_FK, ' +          &#13;&#10;               'KOD_NUM_OUT, ' +                                           // BDC 30.08.2013                                                                                                                                                                                                                                                                                                                               &#13;&#10;               'NAIMOTD, ' +          &#13;&#10;               'KODXIOP       AS KOD, ' +          &#13;&#10;               'NAIMXIOP      AS NAME, ' +&#13;&#10;               'COUNT(*)      AS NUM_OPER, ' +                                                     &#13;&#10;               'SUM(NUM_OSXI) AS NUM_OSXI ' +          &#13;&#10;             //'FROM RP_GB4_09A(:ID_LPU, :KODOTD, :dtStart, :dtStop) ' +   // BDC 30.08.2013            &#13;&#10;             //'FROM RP_GB4_09A(:ID_LPU, :KOD_NUM, :dtStart, :dtStop) ' +  // BDC 30.08.2013 // BDC 18.07.2014              &#13;&#10;               'FROM RP_GB4_09A(:ID_LPU, :KOD_NUM, :dtStart, :dtStop, :VIDOPL) ' +           // BDC 18.07.2014                &#13;&#10;         'WHERE KODXIOP IS NOT NULL ' +                                                                                       &#13;&#10;       //'GROUP BY 1,2,3,4 ' +                                             // BDC 30.08.2013                          &#13;&#10;         'GROUP BY 1,2,3,4,5 ' +                                           // BDC 30.08.2013                          &#13;&#10;       //'ORDER BY 1,3 ';                                                  // BDC 30.08.2013           &#13;&#10;         'ORDER BY 2,4 ';                                                  // BDC 30.08.2013           &#13;&#10;      dsDataB.SQL.Text :=&#13;&#10;        'SELECT ' +           &#13;&#10;               'SPROTD_ID_FK, ' +          &#13;&#10;               'KOD_NUM_OUT, ' +                   // BDC 30.08.2013                                                                                                                                                                                                                                                                                                                               &#13;&#10;               'KODXIOP       AS KOD, ' +          &#13;&#10;               'NAIMXIOP      AS NAME, ' +&#13;&#10;               'NIB, ' +          &#13;&#10;               'FIO, ' +          &#13;&#10;               'KODOSXI, ' +          &#13;&#10;               'NAIMOSXI ' +          &#13;&#10;             //'FROM RP_GB4_09B(:ID_LPU, :KODOTD, :dtStart, :dtStop) ' +   // BDC 30.08.2013  &#13;&#10;             //'FROM RP_GB4_09B(:ID_LPU, :KOD_NUM, :dtStart, :dtStop) ' +  // BDC 30.08.2013 // BDC 18.07.2014    &#13;&#10;               'FROM RP_GB4_09B(:ID_LPU, :KOD_NUM, :dtStart, :dtSto, :VIDOPLp) ' +           // BDC 18.07.2014      &#13;&#10;       //'ORDER BY 5 ';                                                    // BDC 30.08.2013           &#13;&#10;         'ORDER BY 6 ';                                                    // BDC 30.08.2013           &#13;&#10;    end;              &#13;&#10;  dsData.ParamByName('dtStart').Value    := DateToStr(dtBegin.Date);            &#13;&#10;  dsData.ParamByName('dtStop').Value     := DateToStr(dtEnd.Date);&#13;&#10;  dsData.ParamByName('ID_LPU').Value     := StrToInt(VarIdLPU);&#13;&#10;//dsData.ParamByName('KODOTD').Value     := kod;                           // BDC 30.08.2013             &#13;&#10;  dsData.ParamByName('KOD_NUM').Value    := kod;                           // BDC 30.08.2013             &#13;&#10;  // Вид оплаты                                                            // BDC 18.07.2014                              &#13;&#10;  if ComboBox1.ItemIndex &#62; -1 then                                         // BDC 18.07.2014&#13;&#10;    begin                                                                  // BDC 18.07.2014                           &#13;&#10;      Set('VidOpl', '''' + 'вид оплаты: ' + ComboBox1.Text + '''');        // BDC 18.07.2014&#13;&#10;      dsData.ParamByName('vidopl').Value := ComboBox1.ItemIndex + 1        // BDC 18.07.2014                           &#13;&#10;    end                                                                    // BDC 18.07.2014              &#13;&#10;  else                                                                     // BDC 18.07.2014&#13;&#10;    begin                                                                  // BDC 18.07.2014                           &#13;&#10;      Set('VidOpl', '''' + 'все виды оплаты' + '''');                      // BDC 18.07.2014&#13;&#10;      dsData.ParamByName('vidopl').Value := 0;                             // BDC 18.07.2014    &#13;&#10;    end;              &#13;&#10;  dsData.Open;                       &#13;&#10;&#13;&#10;  dsDataB.ParamByName('dtStart').Value    := DateToStr(dtBegin.Date);            &#13;&#10;  dsDataB.ParamByName('dtStop').Value     := DateToStr(dtEnd.Date);&#13;&#10;  dsDataB.ParamByName('ID_LPU').Value     := StrToInt(VarIdLPU);&#13;&#10;//dsDataB.ParamByName('KODOTD').Value     := kod;                          // BDC 30.08.2013               &#13;&#10;  dsDataB.ParamByName('KOD_NUM').Value    := kod;                          // BDC 30.08.2013               &#13;&#10;  // Вид оплаты                                                            // BDC 18.07.2014                              &#13;&#10;  if ComboBox1.ItemIndex &#62; -1 then                                         // BDC 18.07.2014&#13;&#10;    begin                                                                  // BDC 18.07.2014                           &#13;&#10;      Set('VidOpl', '''' + 'вид оплаты: ' + ComboBox1.Text + '''');        // BDC 18.07.2014&#13;&#10;      dsDataB.ParamByName('vidopl').Value := ComboBox1.ItemIndex + 1       // BDC 18.07.2014                           &#13;&#10;    end                                                                    // BDC 18.07.2014              &#13;&#10;  else                                                                     // BDC 18.07.2014&#13;&#10;    begin                                                                  // BDC 18.07.2014                           &#13;&#10;      Set('VidOpl', '''' + 'все виды оплаты' + '''');                      // BDC 18.07.2014&#13;&#10;      dsDataB.ParamByName('vidopl').Value := 0;                            // BDC 18.07.2014    &#13;&#10;    end;              &#13;&#10;  dsDataB.Open;                       &#13;&#10;&#13;&#10;  if dsData.RecordCount = 0 then&#13;&#10;    begin                                                            &#13;&#10;      ShowMessage('Нет данных согласно выбранным условиям!');                                                          &#13;&#10;    end                                                                   &#13;&#10;  else                                                                    &#13;&#10;    begin                                                                 &#13;&#10;      Button1.ModalResult := mrOk;                                                                                &#13;&#10;      fMain.modalresult   := mrOk;                                            &#13;&#10;    end;                                                                                &#13;&#10;end;&#13;&#10;&#13;&#10;procedure fMainOnActivate(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  dtBegin.Date := EncodeDate(YearOf(Now),MonthOf(Now),1);&#13;&#10;  dtEnd.Date := EncodeDate(YearOf(now),MonthOf(now),DaysInMonth(YearOf(now),MonthOf(now)));&#13;&#10;  &#13;&#10;  dsOtd.Close;&#13;&#10;  dsOtd.ParamByName('lpu_id').Value := StrToInt(VarIdLPU);&#13;&#10;  dsOtd.Open;&#13;&#10;  dsOtd.Last;&#13;&#10;  dsOtd.First;          &#13;&#10;{  &#13;&#10;  Memo54.Color  := $AFEEEE; // бледно бирюзовый - получился желто-коричневый&#13;&#10;  Memo54A.Color := $AFEEEE;                                                    &#13;&#10;  Memo25.Color  := $AFEEEE;                                                    &#13;&#10;  Memo28.Color  := $AFEEEE;                                                    &#13;&#10;  Memo54.Color  := $87CEFA; // небесно голубой светлый  - получился коричнево-красный :-(                                                                                                                                                        &#13;&#10;  Memo54A.Color := $87CEFA;                                                    &#13;&#10;  Memo25.Color  := $87CEFA;                                                    &#13;&#10;  Memo28.Color  := $87CEFA;                                                    &#13;&#10;  Memo54.Color  := $B7CEFA; // а так розоватый                                                                                                                                                                                        &#13;&#10;  Memo54A.Color := $B7CEFA;                                                    &#13;&#10;  Memo25.Color  := $B7CEFA;                                                    &#13;&#10;  Memo28.Color  := $B7CEFA;                                                    &#13;&#10;  Memo54.Color  := $F0FFFF; // лазурный - получился желтый                                                                                                                                                                                                                                 &#13;&#10;  Memo54A.Color := $F0FFFF;                                                    &#13;&#10;  Memo25.Color  := $F0FFFF;                                                    &#13;&#10;  Memo28.Color  := $F0FFFF;&#13;&#10;    &#13;&#10;  Memo54.Color  := $FFFFF0; // лазурный                                                                                                                                                                                        &#13;&#10;  Memo54A.Color := $FFFFF0;                                                    &#13;&#10;  Memo25.Color  := $FFFFF0;                                                    &#13;&#10;  Memo28.Color  := $FFFFF0;&#13;&#10;  Memo54.Color  := $EEEEAF; // бледно бирюзовый                                                                                                                                                           &#13;&#10;  Memo54A.Color := $EEEEAF;                                                    &#13;&#10;  Memo25.Color  := $EEEEAF;                                                    &#13;&#10;  Memo28.Color  := $EEEEAF;                                                    &#13;&#10;  Memo54.Color  := $FACE87; // небесно голубой светлый                                                                                                                                                        &#13;&#10;  Memo54A.Color := $FACE87;                                                    &#13;&#10;  Memo25.Color  := $FACE87;                                                    &#13;&#10;  Memo28.Color  := $FACE87;                                                    &#13;&#10;}  &#13;&#10;  Memo54.Color  := $F5F5F5; // дымчатый&#13;&#10;  Memo54A.Color := $F5F5F5;                                                    &#13;&#10;  Memo25.Color  := $F5F5F5;                                                    &#13;&#10;  Memo28.Color  := $F5F5F5;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure mdOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;//dsDataB.Filter := 'KOD= ''' + dsdata.FieldByName('KOD').AsString + ''' AND SPROTD_ID_FK= ''' + dsdata.FieldByName('SPROTD_ID_FK').AsString + '''';&#13;&#10;  dsDataB.Filter := 'KOD= ''' + dsdata.FieldByName('KOD').AsString + ''' AND KOD_NUM_OUT= '''  + dsdata.FieldByName('KOD_NUM_OUT').AsString + '''';&#13;&#10;//ShowMessage(dsDataB.Filter);                                &#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;procedure btSubOnClick(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  D, M, Y: word;&#13;&#10;  LastDay: boolean;&#13;&#10;begin&#13;&#10;  DecodeDate(dtBegin.Date,Y,M,D);&#13;&#10;  LastDay := (D=DaysInMonth(Y,M));&#13;&#10;  M := M - 1;&#13;&#10;  if M = 0 then&#13;&#10;  begin&#13;&#10;    Y := Y - 1;&#13;&#10;    M := 12;&#13;&#10;  end;&#13;&#10;  if LastDay then&#13;&#10;    D := DaysInMonth(Y,M)&#13;&#10;  else&#13;&#10;  if D&#62;DaysInMonth(Y,M) then&#13;&#10;    D := DaysInMonth(Y,M);&#13;&#10;  dtBegin.Date := EncodeDate(Y,M,D);&#13;&#10;&#13;&#10;  DecodeDate(dtEnd.Date,Y,M,D);&#13;&#10;  LastDay := (D=DaysInMonth(Y,M));&#13;&#10;  M := M - 1;&#13;&#10;  if M = 0 then&#13;&#10;  begin&#13;&#10;    Y := Y - 1;&#13;&#10;    M := 12;&#13;&#10;  end;&#13;&#10;  if LastDay then&#13;&#10;    D := DaysInMonth(Y,M)&#13;&#10;  else&#13;&#10;  if D&#62;DaysInMonth(Y,M) then&#13;&#10;    D := DaysInMonth(Y,M);&#13;&#10;  dtEnd.Date := EncodeDate(Y,M,D);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure btAddOnClick(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  D, M, Y: word;&#13;&#10;  LastDay: boolean;&#13;&#10;begin&#13;&#10;  DecodeDate(dtBegin.Date,Y,M,D);&#13;&#10;  LastDay := (D=DaysInMonth(Y,M));&#13;&#10;  M := M + 1;&#13;&#10;  if M = 13 then&#13;&#10;  begin&#13;&#10;    Y := Y + 1;&#13;&#10;    M := 1;&#13;&#10;  end;&#13;&#10;  if LastDay then&#13;&#10;    D := DaysInMonth(Y,M)&#13;&#10;  else&#13;&#10;  if D&#62;DaysInMonth(Y,M) then&#13;&#10;    D := DaysInMonth(Y,M);&#13;&#10;  dtBegin.Date := EncodeDate(Y,M,D);&#13;&#10;&#13;&#10;  DecodeDate(dtEnd.Date,Y,M,D);&#13;&#10;  LastDay := (D=DaysInMonth(Y,M));&#13;&#10;  M := M + 1;&#13;&#10;  if M = 13 then&#13;&#10;  begin&#13;&#10;    Y := Y + 1;&#13;&#10;    M := 1;&#13;&#10;  end;&#13;&#10;  if LastDay then&#13;&#10;    D := DaysInMonth(Y,M)&#13;&#10;  else&#13;&#10;  if D&#62;DaysInMonth(Y,M) then&#13;&#10;    D := DaysInMonth(Y,M);&#13;&#10;  dtEnd.Date := EncodeDate(Y,M,D);&#13;&#10;end;&#13;&#10;       &#13;&#10;begin&#13;&#10;&#13;&#10;end.&#13;&#10;    " StoreInDFM="False" OnReportPrint="frxReportOnReportPrint" PropData="">
