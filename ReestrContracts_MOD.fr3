    <TfrxFIBQuery Name="dsContract" UserName="dsContract" CloseDataSource="True" FieldAliases.Text="CONTRACT_ID=CONTRACT_ID&#13;&#10;CONTRACT_NAME=CONTRACT_NAME&#13;&#10;ID_LPU_FK=ID_LPU_FK&#13;&#10;LPU_NAME=LPU_NAME&#13;&#10;BEGIN_DATE=BEGIN_DATE&#13;&#10;END_DATE=END_DATE&#13;&#10;CONTRACT_NUMBER=CONTRACT_NUMBER&#13;&#10;ORGANIZATION=ORGANIZATION&#13;&#10;UTV_DOLJ=UTV_DOLJ&#13;&#10;UTV_FIO=UTV_FIO&#13;&#10;COMMENTS=COMMENTS&#13;&#10;COST=COST&#13;&#10;PS_INN=PS_INN&#13;&#10;PS_KPP=PS_KPP" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT *&#13;&#10;  FROM vw_contracts con&#13;&#10; WHERE con.id_lpu_fk   = :ID_LPU&#13;&#10; --  AND con.contract_id = :CONTRACT_ID&#13;&#10;                 " PropData="05704C65667403BC000470546F7002500A506172616D657465727301010623204E616D653D2249445F4C5055222044617461547970653D226674556E6B6E6F776E2200010628204E616D653D22434F4E54524143545F4944222044617461547970653D226674556E6B6E6F776E220000"/>
    <TfrxFIBQuery Name="dsReestr" UserName="dsReestr" CloseDataSource="True" FieldAliases.Text="KOD_NUM=KOD_NUM&#13;&#10;NAIMOTD=NAIMOTD&#13;&#10;FIO=FIO&#13;&#10;BIRTHDAY=BIRTHDAY&#13;&#10;MROD=MROD&#13;&#10;WORK_PLACE=WORK_PLACE&#13;&#10;CERTIFICATE=CERTIFICATE&#13;&#10;DIRECT_LPU_NUMDIRECT=DIRECT_LPU_NUMDIRECT&#13;&#10;DIRECT_LPU_DTDIRECT=DIRECT_LPU_DTDIRECT&#13;&#10;DIAGNOS=DIAGNOS&#13;&#10;NAIMDIAG=NAIMDIAG&#13;&#10;CLINIC_ACCOMPDIAGS=CLINIC_ACCOMPDIAGS&#13;&#10;RESULT_HOSP=RESULT_HOSP&#13;&#10;RMPNAME=RMPNAME&#13;&#10;DTRECEIPT=DTRECEIPT&#13;&#10;DTEXTRACT=DTEXTRACT&#13;&#10;KD=KD&#13;&#10;CL_OTD=CL_OTD" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="   SELECT pm.sprotd_id_fk * 10 + COALESCE(pm.otd_num, 0) AS KOD_NUM,&#13;&#10;          otd.naimotd || COALESCE(' № ' || CAST(pm.otd_num AS VARCHAR(2)), '') AS NAIMOTD,&#13;&#10;          peo.lname || ' '  || peo.fname || COALESCE(' ' || peo.mname, '') AS FIO,&#13;&#10;          peo.birthday,&#13;&#10;          peo.mrod,&#13;&#10;          pat.work_place,&#13;&#10;          pat.certificate,&#13;&#10;          pat.direct_lpu_numdirect,&#13;&#10;          pat.direct_lpu_dtdirect,&#13;&#10;          pm.diagnos,&#13;&#10;          mkb.naimdiag,&#13;&#10;          pat.clinic_accompdiags,&#13;&#10;          pat.result_hosp,&#13;&#10;          res.rmpname,&#13;&#10;          pm.dtreceipt,&#13;&#10;          pm.dtextract,&#13;&#10;          pm.kd,&#13;&#10;          pm.cl_otd&#13;&#10;     FROM wt_patient     pat&#13;&#10;     JOIN wt_patient_move pm ON pm.wt_patient_id_fk = pat.wt_patient_id&#13;&#10;     JOIN vw_st_sprotd   otd ON otd.kodotd          = pm.sprotd_id_fk&#13;&#10;LEFT JOIN vw_diagnosis   mkb ON mkb.koddiag         = pm.diagnos&#13;&#10;     JOIN vw_st_bol      bol ON bol.kodbol          = pat.id_lpu_fk&#13;&#10;LEFT JOIN v009$rezult    res ON res.idrmp           = pat.result_hosp&#13;&#10;     JOIN peoples_loc    peo ON peo.people_id       = pat.people_id_fk&#13;&#10;    WHERE pat.dtextract BETWEEN :DATE_BEG AND :DATE_END&#13;&#10;      AND pm.contract_id_fk = :CONTRACT_ID&#13;&#10;      AND bol.key3          = :ID_LPU&#13;&#10;      AND pm.cl_otd         &#62; 0&#13;&#10;    ORDER BY&#13;&#10;          kod_num,&#13;&#10;          fio,&#13;&#10;          dtreceipt,&#13;&#10;          dtextract              &#13;&#10;&#13;&#10;&#13;&#10;    " PropData=""/>
