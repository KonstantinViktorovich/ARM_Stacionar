<TfrxReport Version="4.8.27" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="40720,5875882407" ReportOptions.Description.Text="" ReportOptions.LastChange="41949,4494553356" ScriptLanguage="PascalScript" ScriptText.Text="&#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);&#13;&#10;Var&#13;&#10;S : String;&#13;&#10;begin&#13;&#10;If DBlcOtd.KeyValue=Null Then&#13;&#10; Begin&#13;&#10;  ShowMessage('Не указано отделение');                                     &#13;&#10;  Button1.ModalResult:=mrNone;                                        &#13;&#10;  fMain.modalresult:=mrNone;&#13;&#10;  Exit;              &#13;&#10; End;  &#13;&#10;  S := 'за период';&#13;&#10;  S := S + ' с ' + DateToStr(dtBegin.Date);        &#13;&#10;  S := S + ' по ' + DateToStr(dtEnd.Date);&#13;&#10;  Set('PeriodName', ''''+S+'''');&#13;&#10;  Set('NameOtd', ''''+dsOtd.FieldByName('FullName').AsString+'''');&#13;&#10;  dsLPU.Close;&#13;&#10;  dsLPU.parambyname('kodbol').Value := StrToInt(VarIdLPU);&#13;&#10;  dsLPU.Open;          &#13;&#10;  Set('NameMO', ''''+dsLPU.FieldByName('st_bol_name').AsString+'''');&#13;&#10;  dsLPU.Close;&#13;&#10;  dsData.Close;                  &#13;&#10;  dsData.parambyname('dtStart').value:= DateToStr(dtBegin.Date);            &#13;&#10;  dsData.parambyname('dtStop').value:=DateToStr(dtEnd.Date);&#13;&#10;//dsData.parambyname('otd').value:=dsOtd.FieldByName('KODOTD').AsString;&#13;&#10;  dsData.ParamByName('KOD_NUM').Value := dsOtd.FieldByName('KOD_NUM').AsString;&#13;&#10;  dsData.Open;&#13;&#10;    &#13;&#10;  dsWithoutResult.Close;                      &#13;&#10;  dsWithoutResult.parambyname('lpu_id').Value := StrToInt(VarIdLPU);&#13;&#10;  dsWithoutResult.parambyname('dtStart').value:= DateToStr(dtBegin.Date);            &#13;&#10;  dsWithoutResult.parambyname('dtStop').value:=DateToStr(dtEnd.Date);      &#13;&#10;  dsWithoutResult.parambyname('otd').value:=dsOtd.FieldByName('KOD_NUM').AsString;&#13;&#10;  dsWithoutResult.Open;&#13;&#10;    &#13;&#10;  Button1.ModalResult:=mrOk;                                        &#13;&#10;  fMain.modalresult:=mrOk;    &#13;&#10;end;&#13;&#10;&#13;&#10;procedure fMainOnActivate(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  dtBegin.Date := EncodeDate(YearOf(now),MonthOf(now)-1,1);&#13;&#10;  dtEnd.Date := EncodeDate(YearOf(now),MonthOf(now)-1,DaysInMonth(YearOf(now),MonthOf(now)-1));&#13;&#10;&#13;&#10;  dsOtd.Close;&#13;&#10;  dsOtd.parambyname('lpu_id').Value := StrToInt(VarIdLPU);&#13;&#10;  dsOtd.Open;&#13;&#10;  dsOtd.Last;&#13;&#10;  dsOtd.First;          &#13;&#10;end;&#13;&#10;&#13;&#10;procedure btSubOnClick(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  D, M, Y: word;&#13;&#10;  LastDay: boolean;&#13;&#10;begin&#13;&#10;  DecodeDate(dtBegin.Date,Y,M,D);&#13;&#10;  LastDay := (D=DaysInMonth(Y,M));&#13;&#10;  M := M - 1;&#13;&#10;  if M = 0 then&#13;&#10;  begin&#13;&#10;    Y := Y - 1;&#13;&#10;    M := 12;&#13;&#10;  end;&#13;&#10;  if LastDay then&#13;&#10;    D := DaysInMonth(Y,M)&#13;&#10;  else&#13;&#10;  if D&#62;DaysInMonth(Y,M) then&#13;&#10;    D := DaysInMonth(Y,M);&#13;&#10;  dtBegin.Date := EncodeDate(Y,M,D);&#13;&#10;&#13;&#10;  DecodeDate(dtEnd.Date,Y,M,D);&#13;&#10;  LastDay := (D=DaysInMonth(Y,M));&#13;&#10;  M := M - 1;&#13;&#10;  if M = 0 then&#13;&#10;  begin&#13;&#10;    Y := Y - 1;&#13;&#10;    M := 12;&#13;&#10;  end;&#13;&#10;  if LastDay then&#13;&#10;    D := DaysInMonth(Y,M)&#13;&#10;  else&#13;&#10;  if D&#62;DaysInMonth(Y,M) then&#13;&#10;    D := DaysInMonth(Y,M);&#13;&#10;  dtEnd.Date := EncodeDate(Y,M,D);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure btAddOnClick(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  D, M, Y: word;&#13;&#10;  LastDay: boolean;&#13;&#10;begin&#13;&#10;  DecodeDate(dtBegin.Date,Y,M,D);&#13;&#10;  LastDay := (D=DaysInMonth(Y,M));&#13;&#10;  M := M + 1;&#13;&#10;  if M = 13 then&#13;&#10;  begin&#13;&#10;    Y := Y + 1;&#13;&#10;    M := 1;&#13;&#10;  end;&#13;&#10;  if LastDay then&#13;&#10;    D := DaysInMonth(Y,M)&#13;&#10;  else&#13;&#10;  if D&#62;DaysInMonth(Y,M) then&#13;&#10;    D := DaysInMonth(Y,M);&#13;&#10;  dtBegin.Date := EncodeDate(Y,M,D);&#13;&#10;&#13;&#10;  DecodeDate(dtEnd.Date,Y,M,D);&#13;&#10;  LastDay := (D=DaysInMonth(Y,M));&#13;&#10;  M := M + 1;&#13;&#10;  if M = 13 then&#13;&#10;  begin&#13;&#10;    Y := Y + 1;&#13;&#10;    M := 1;&#13;&#10;  end;&#13;&#10;  if LastDay then&#13;&#10;    D := DaysInMonth(Y,M)&#13;&#10;  else&#13;&#10;  if D&#62;DaysInMonth(Y,M) then&#13;&#10;    D := DaysInMonth(Y,M);&#13;&#10;  dtEnd.Date := EncodeDate(Y,M,D);&#13;&#10;end;&#13;&#10;  &#13;&#10;&#13;&#10;       &#13;&#10;begin&#13;&#10;&#13;&#10;end." StoreInDFM="False" OnReportPrint="frxReportOnReportPrint" PropData="">
  <TfrxDataPage Name="Data" Height="1000" Left="0" Top="0" Width="1000">
    <TfrxFIBQuery Name="dsData" UserName="dsData" CloseDataSource="True" FieldAliases.Text="DIAG=DIAG&#13;&#10;K_ALL=K_ALL&#13;&#10;K_1Y=K_1Y&#13;&#10;IZ_STAC=IZ_STAC&#13;&#10;DR_OTD=DR_OTD&#13;&#10;VYPIS=VYPIS&#13;&#10;PEREVOD=PEREVOD&#13;&#10;UM=UM&#13;&#10;NEZAK=NEZAK" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text=" Select &#13;&#10;        Left(DIAG||'     ',7)||NAIMDIAG diag,&#13;&#10;        Count(*) k_all,  &#13;&#10;        SUM(CASE WHEN R_YEAR&#60;1 THEN 1 ELSE 0 END) k_1y,  -- BDC 27.12.2012&#13;&#10;        SUM(CASE WHEN iz_stac=1 THEN 1 ELSE 0 END) iz_stac,      -- BDC 20.09.2013                                                                                                                                                                                                &#13;&#10;        sum(case when dr_otd=1 then 1 else 0 end) dr_otd,  &#13;&#10;        sum(case when vypis=1 then 1 else 0 end) vypis,  &#13;&#10;        sum(case when perevod=1 then 1 else 0 end) perevod,  &#13;&#10;        sum(case when um=1 then 1 else 0 end) um,  &#13;&#10;        sum(case when nezak=1 then 1 else 0 end) nezak        &#13;&#10;   From RP$FORM37&#13;&#10;--Where dtextract&#62;=:dtStart and dtextract&#60;=:dtStop and Otd=:Otd  -- BDC 27.08.2013                                             &#13;&#10;  WHERE dtextract BETWEEN :dtStart AND :dtStop                   -- BDC 27.08.2013  &#13;&#10;    AND KOD_NUM = :KOD_NUM                                       -- BDC 27.08.2013                                                               &#13;&#10;  Group by 1    &#13;&#10;               " PropData=""/>
